{
  "name": "black-owned-biz",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "env-cmd": "^10.1.0",
    "firebase": "^7.15.0",
    "http-proxy-middleware": "^1.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-firebaseui": "^4.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-usa-map": "^1.4.0"
  },
  "scripts": {
    "start": "run-p --race start:firebase start:react",
    "start:firebase": "firebase use default && firebase emulators:start --import=./data --only hosting,firestore",
    "start:react": "react-scripts start",
    "deploy": "firebase use default && npm run build && firebase deploy",
    "deploy:staging": "firebase use staging && npm run build:staging && firebase deploy",
    "build": "react-scripts build",
    "build:staging": "env-cmd -f .env.staging npm run build",
    "export": "firebase emulators:export ./data/",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
