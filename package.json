{
  "name": "black-owned-biz",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.10",
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "env-cmd": "^10.1.0",
    "firebase": "^7.15.0",
    "fontsource-roboto": "^2.1.4",
    "http-proxy-middleware": "^1.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-firebaseui": "^4.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-usa-map": "^1.4.0"
  },
  "scripts": {
    "start": "run-p --race start:firebase start:react",
    "start:firebase": "firebase emulators:start --import=./data --only firestore",
    "start:react": "react-scripts start",
    "start:staging": "env-cmd -f .env.staging npm run start:react",
    "deploy": "firebase use default && npm run build && firebase deploy",
    "deploy:staging": "firebase use staging && npm run build:staging && firebase deploy",
    "build": "react-scripts build",
    "build:staging": "env-cmd -f .env.staging npm run build",
    "export": "firebase emulators:export ./data/",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@react-theming/storybook-addon": "^1.0.2",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.0.1",
    "@storybook/react": "^5.3.19",
    "npm-run-all": "^4.1.5",
    "storybook-react-router": "^1.0.8"
  }
}
